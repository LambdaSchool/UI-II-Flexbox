cing: 1px;\r\n\tmargin-bottom: 15px;\r\n}\r\n\r\n/* Copy and paste your work from yesterday here and start to refactor into flexbox */\r\n\r\nheader {\r\n\tmargin: 0 auto;\r\n}\r\nnav.main-nav {\r\n\twidth: 100vw;\r\n\theight: 80px;\r\n\tposition: fixed;\r\n\tz-index: 99;\r\n\tdisplay: flex;\r\n\tjustify-content: space-between;\r\n\talign-items: center;\r\n\tbackground: white;\r\n\tpadding: 20px 50px;\r\n\tbox-shadow: 0px 15px 20px -15px #111;\r\n}\r\n.main-nav a {\r\n\tpadding: 2px 5px;\r\n\ttext-decoration: none;\r\n\tcolor: gray;\r\n\tborder-bottom: 2px solid transparent;\r\n\ttransition: border-bottom .5s ease-in-out;\r\n}\r\n.main-nav a:hover {\r\n\tborder-bottom: 2px solid gray;\r\n}\r\n.main-header {\r\n\tmargin: 0 auto;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\tpadding-top: 120px;\r\n\tpadding-bottom: 80px;\r\n\talign-items: center;\r\n}\r\n.main-header .slogan {\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n}\r\n.main-header .slogan h1 {\r\n\tfont-family: 'Righteous', cursive;\r\n\tfont-size: 5rem;\r\n\ttext-align: center;\r\n}\r\n.main-header .slogan button {\r\n\tcolor: black;\r\n\tbackground: white;\r\n\tborder: 1px solid black;\r\n\twidth: 250px;\r\n\talign-self: center;\r\n}\r\n.main-header .header-img .circle-img img {\r\n\theight: 100%;\r\n\tborder-radius: 190px;\r\n\tbox-shadow: 0px 25px 30px -15px #111;\r\n}\r\nmain {\r\n\tmargin: 0 auto;\r\n}\r\n.top-content {\r\n\tborder-top: 1px solid black;\r\n\tmargin: 0 50px;\r\n\tpadding: 40px 15px;\r\n\tdisplay: flex;\r\n\tjustify-content: space-around;\r\n}\r\n.top-content .box {\r\n\tpadding: 0 10px;\r\n}\r\n\r\n.middle-img {\r\n\tdisplay: flex;\r\n}\r\n.middle-img img {\r\n\tbox-shadow: 0 8px 12px gray;\r\n\tmargin: 0 auto;\r\n}\r\n.bottom-content {\t\r\n\tborder-bottom: 1px solid black;\r\n\tmargin: 0 50px;\r\n\tpadding: 30px 15px;\r\n\tdisplay: flex;\r\n\tjustify-content: space-around;\r\n}\r\n\r\n.bottom-content .box {\r\n\tpadding: 0 10px;\r\n}\r\n\r\nfooter {\r\n\tpadding: 40px 50px;\r\n}\r\nfooter h2 {\r\n\tpadding-left: 20px;\r\n}\r\nfooter address p {\r\n\tpadding: 5px 0 0 20px;\r\n}\r\n\r\n.legal p {\r\n\tmargin-top: 60px;\r\n\ttext-align: center;\r\n}\r\n\r\n/* Service Page */\r\n.services-header {\r\n\twidth: 100%;\r\n\tmargin: 0 auto;\r\n\tpadding: 0 50px 20px 50px;\r\n\t\r\n}\r\n.services-header .services-header-img {\r\n\twidth: 100%;\r\n\tmargin-top: 80px;\r\n}\r\n.services-header h1 {\r\n\tpadding: 20px 0 5px 0;;\r\n}\r\n.services-header p {\r\n\tpadding-bottom: 30px;\r\n\tborder-bottom: 1px solid black;\r\n\tline-height: 1.2;\r\n}\r\n\r\n.services-container{\r\n\tmargin: 0 auto;\r\n\twidth:100%;\r\n\tdisplay: flex;\r\n\tflex-wrap: wrap;\r\n\tjustify-content: center;\r\n}\r\n.services-container .box {\r\n\twidth: 430px;\r\n\theight: 215px;\r\n\tpadding: 20px;\r\n\tmargin: 20px;\r\n\tborder: 1px solid black;\r\n\tbackground: lightgray;\r\n}\r\n.services-container .box p {\r\n\tpadding-bottom: 15px;\r\n}\r\n.services-container .box button {\r\n\tcolor: #31302B;\r\n    background: #FFF;\r\n    padding: 8px 17px;\r\n    border: 1px solid #31302B;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n    letter-spacing: 1px;\r\n    text-transform: uppercase;\r\n    border-radius: 5px;\r\n    text-align: center;\r\n    box-shadow: inset 0 0 0 0 #31302B;\r\n\ttransition: all ease 0.8s;\r\n}\r\n.services-container .box button:hover {\r\n\tbox-shadow: inset 100px 0 0 0 #e0e0e0;\r\n    color:green;\r\n}\r\n.footer-content {\r\n\tborder-top: 1px solid black;\r\n\tpadding-top: 30px;\r\n\tdisplay: flex;\r\n}\r\n.footer-box {\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n\tpadding-right: 50px;\r\n}\r\nul.footer-list {\r\n\tpadding-top: 10px;\r\n\tlist-style-type: disc; \r\n\tlist-style-position: inside;\r\n}"},"lineCount":234}},"error":null,"hash":"567b9d961077a524a9c89ae3c07b36f0","cacheData":{"env":{}}}